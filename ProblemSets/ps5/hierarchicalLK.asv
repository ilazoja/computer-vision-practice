function [U,V] = hierarchicalLK(L, R, k)

    gauss_pyr(1,:) = gaussPyramid(double(L),k);
    gauss_pyr(2,:) = gaussPyramid(double(R),k);
    
    Lk = gauss_pyr(1,end);
    Rk = gauss_pyr(2,end);
    
    
    
    for i = k:-1:0
        if i == k
            U = zeros(size(Lk));
            V = zeros(size(Lk));
        else
            Uip = expand(Uk)l
        end
    end
    

    
end